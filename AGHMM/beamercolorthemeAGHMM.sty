\setbeamercolor{palette tertiary}{fg=AGH@white}  %{fg=AGH@white,bg=AGH@white}

\setbeamercolor{title}{fg=AGH@white, bg=AGH@green} % title of the slides
\setbeamercolor{titlelike}{fg=AGH@white, bg=AGH@green} % title of the sections
\setbeamercolor{frametitle}{fg=AGH@white, bg=AGH@green} % title of the frames
\setbeamercolor{part title}{fg=AGH@white} % title of the parts
\setbeamercolor{section in toc}{fg=AGH@black, bg=AGH@black} % TOC sections
